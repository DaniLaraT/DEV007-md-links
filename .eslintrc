{
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "extends": "eslint:recommended",
    "rules": {
          "indent": ["error", 2],
          "linebreak-style": ["error", "windows"],
          "quotes": ["error", "single"],
          "semi": ["error", "always"]
        }
  }